    <application
        android:name="com.assignmentmain.MyApplication"
        android:icon="@drawable/assignment" >
        <activity
            android:name="com.assignmentmain.MovieListActivity"
            android:label="Assignment mobisys" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
